<!-- Modal -->
<div class="modal fade" id="SubmitIdeaModel" tabindex="-1" role="dialog" aria-labelledby="SubmitIdeaModelLabel" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="SubmitIdeaModelLabel">Are you sure?</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <p class="alert alert-blue">Once you submit your idea, you can no longer edit it.  Are you ready to submit your idea to the Imagination Workgroup?</p>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                <button type="button" data-bind='click: submitIdea, enable: SubmitEnabled, text:SubmitText' class="btn btn-primary"></button>
            </div>
        </div>
    </div>
</div>
